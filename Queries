// PERSONAL DETAILS TABLE CREATION

CREATE TABLE TEAM1.PERSONAL_DETAILS
(CUSTOMER_ID INTEGER NOT NULL,
CUSTOMER_NAME VARCHAR(25),
AGE INTEGER,
GENDER VARCHAR(25),
ZIP_CODE INTEGER,
MARRIED VARCHAR(25),
NUMBER_OF_CHILDREN INTEGER,
INCOME INTEGER,
FREQUENCY_OF_VISITS VARCHAR(25),
SYSDATE VARCHAR(25),
PRIMARY KEY (CUSTOMER_ID))

// HEALTH_AND_HABITS TABLE CREATION

CREATE TABLE TEAM1.HEALTH_AND_HABITS
(CUSTOMER_ID INTEGER,
CARDIAC_DISEASE VARCHAR(25),
ALLERGY VARCHAR(25),
DIABETES VARCHAR(25),
CANCER VARCHAR(25),
MENTAL_HEALTH VARCHAR(25),
ALCOHOL VARCHAR(25),
CIGARETTE VARCHAR(25))

// SERVICE PREFERENCES TABLE CREATION

CREATE TABLE TEAM1.PREFERENCES_SERVICES
(CUSTOMER_ID INTEGER NOT NULL,
SERVICE_ABORTION INTEGER,
SERVICE_ACUPUNCTURE INTEGER,
SERVICE_BABYCARE INTEGER,
SERVICE_CANCER INTEGER,
SERVICE_CHIROPRACTIC_CARE INTEGER,
SERVICE_COSMETIC_SURGERY INTEGER,
SERVICE_DENTAL INTEGER,
SERVICE_DIABETES INTEGER,
SERVICE_EYECARE INTEGER,
SERVICE_HABILITATION INTEGER,
SERVICE_HOSPICE INTEGER,
SERVICE_IMMUNIZATION INTEGER,
SERVICE_INFERTILITY INTEGER,
SERVICE_MAMMOGRAM INTEGER,
SERVICE_NURSING INTEGER,
SERVICE_POSTNATAL INTEGER,
SERVICE_PRENATAL INTEGER,
SERVICE_REFERRAL INTEGER,
SERVICE_REHABILITATION INTEGER,
SERVICE_SURGERY INTEGER,
SERVICE_URGENT_CARE INTEGER,
SERVICE_WEIGHT_LOSS INTEGER,
SERVICE_X_RAY INTEGER,
SCORE INTEGER,
PRIMARY KEY (CUSTOMER_ID))

// COST PREFERENCES TABLE

CREATE TABLE TEAM1.PREFERENCES_COST
(CUSTOMER_ID INTEGER NOT NULL,
PREMIUM INTEGER,
DEDUCTIBLE INTEGER,
PREMIUM_WEIGHT INTEGER,
DEDUCTIBLE_WEIGHT INTEGER,
PRIMARY KEY (CUSTOMER_ID))
